{
  "name": "bartaz-icons",
  "version": "1.2.1",
  "scripts": {
    "clean": "rimraf dist && rimraf src",
    "build": "yarn convert && tsup src/index.tsx --format cjs,esm --minify --dts --out-dir dist",
    "convert": "svgr ./icons --out-dir ./src --ext tsx --typescript --icon --replace-attr-values '#202939=currentColor'"
  },
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "author": "Zeynal Zeynalli",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZeynalliZeynal/bart-az-icons.git"
  },
  "devDependencies": {
    "@preconstruct/cli": "^2.8.10",
    "@svgr/cli": "^8.1.0",
    "@swc/core": "^1.10.11",
    "@types/node": "^22.12.0",
    "@types/react": "^19.0.8",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "xml-js": "^1.6.11"
  },
  "peerDependencies": {
    "react": "^16.x || ^17.x || ^18.x || ^19.0.0 || ^19.0.0-rc"
  }
}
